<h1>Welcome to this chat application</h1>
<form method="POST">
  <input type="text" name="sender" />
  <input type="text" name="message" />
  <input type="submit" value="Submit!" />
</form>

<ul>
<% it.messages.forEach((message) => { %>
<li><%= message.sender %> said: "<%= message.message %>"</li>
<% }); %>
</ul>